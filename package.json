{
  "name": "sketches",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:eleventy": "eleventy --serve",
    "dev:js": "esbuild --watch src/js/entries/* --format=iife --bundle --outdir=_site --outbase=src/js  --sourcemap --minify --global-name=sketch",
    "dev:tsc": "tsc -noEmit",
    "prod:eleventy": "eleventy",
    "prod:js": "tsc -noEmit && esbuild src/js/entries/*  --format=iife --bundle --outdir=_site --outbase=src/js --minify --global-name=sketch",
    "build": "run-s prod:*",
    "clean": "rm -rf _site .cache"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@types/p5": "^1.3.0",
    "@types/random": "^3.0.1",
    "date-fns": "^2.23.0",
    "esbuild": "^0.12.22",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/delaunator": "^3.0.0",
    "chance.js": "^0.0.1",
    "delaunator": "^5.0.0",
    "p5": "^1.4.0",
    "p5.createloop": "^0.2.8",
    "p5.js-func": "github:burnto/p5.js-func",
    "random": "^3.0.6",
    "randy": "^1.5.1",
    "sentencer": "^0.2.1"
  }
}
