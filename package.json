{
  "name": "sketches",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:eleventy": "eleventy --serve",
    "dev:js": "esbuild --watch src/js/entries/* src/js/collections/* src/js/load.js src/js/helpers.ts --outdir=_site --outbase=src/js  --sourcemap --minify",
    "dev:tsc": "tsc -noEmit",
    "prod:eleventy": "eleventy",
    "prod:js": "tsc -noEmit && esbuild src/js/entries/* src/js/collections/* src/js/load.js src/js/helpers.ts --outdir=_site --outbase=src/js --minify",
    "build": "run-s prod:*",
    "clean": "rm -rf _site .cache"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@types/p5": "^1.3.0",
    "date-fns": "^2.23.0",
    "esbuild": "^0.12.22",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "p5": "^0.8.0",
    "p5.createloop": "^0.2.8",
    "randy": "^1.5.1",
    "sentencer": "^0.2.1"
  }
}
